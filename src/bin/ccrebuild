#!/usr/bin/env python

import sys
import json

if __name__ == "__main__":
    if len(sys.argv) != 2:
        print >> sys.stderr, "Usage: %s PROFILE" % sys.argv[0]
        sys.exit(1)
    
    filename = sys.argv[1]
    fp = open(filename)
    profile = json.load(fp)
    
    print profile.database
